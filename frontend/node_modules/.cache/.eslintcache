div>\n      </div>\n      <div className=\"edubahamas__navbar-sign\">\n        <Linker to=\"/signIn\">\n          <button type=\"button\" className=\"bg-transparent hover:cursor-pointer hover:text-[#5F9EA0] hover:bg-transparent w-full w-full\">Sign In</button>\n        </Linker>\n        <Linker to=\"/register\">\n          <button className=\"bg-blue-600 w-full hover:bg-blue-800 transition delay-50 ease-in-out\" type=\"button\">Register</button>\n        </Linker>\n      </div>\n      <div className=\"edubahamas__navbar-menu\">\n        {toggleMenu\n          ? <RiCloseLine color=\"#fff\" size={27} onClick={() => setToggleMenu(false)} />\n          : <RiMenu3Line color=\"#fff\" size={27} onClick={() => setToggleMenu(true)} />\n        } \n        {toggleMenu && (\n          <div className=\"edubahamas__navbar-menu-container scale-up-center\">\n            <div className=\"edubahamas__navbar-menu-container-links\">\n              <Menu/>\n              <div className=\"edubahamas__navbar-menu-container-links-sign\">\n                <p>Sign In</p>\n                <button type=\"button\">Register Now</button>\n              </div>\n            </div>\n          </div>\n        )}\n      </div>\n    </div>\n  )\n}\n\nexport default Navbar;","/Users/cathy/Desktop/fall_project/frontend/src/components/section-three/sectionThree.js",["47"],[],"import React from 'react';\nimport { Card } from 'flowbite-react';\nimport individual from '../../assets/individual.svg'\nimport teacher from '../../assets/teacher.svg'\nimport student from '../../assets/student.svg'\n\nconst SectionThree = () => {\n  \n  return (\n    <div>   \n         \n    <div id=\"resources\" className=\"w-full h-screen bg-white flex flex-col items-center justify-center\">\n        <h1 className=\"flex flex-col font-thin text-6xl 2xl:text-8xl mb-[10%]\">Learn and Teach with EduBahamas</h1>\n        <div className=\"grid md:grid-cols-2\">\n\n        <Card horizontal imgSrc={teacher} className=\"mr-[10%] hover: